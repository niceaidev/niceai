{
  "name": "niceai-monorepo",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production turbo build --filter=\"./apps/*\" --force",
    "format": "biome format --fix .",
    "lint": "biome lint --fix .",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:push": "turbo db:push",
    "prepare": "niceai-scripts prepare",
    "setup": "niceai-scripts setup",
    "clean": "niceai-scripts clean",
    "build:docker": "turbo build:docker",
    "start:docker": "turbo start:docker",
    "db:studio": "turbo db:studio",
    "start": "turbo run start --filter=\"./apps/*\"",
    "test": "vitest",
    "changeset": "changeset",
    "coverage": "vitest --coverage",
    "dev:pkg": "NODE_ENV=development turbo dev --filter=\"./packages/*\"",
    "dev": "NODE_ENV=development turbo dev --filter=\"./apps/*\" --parallel"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.9",
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@niceai/cli": "workspace:*",
    "@types/node": "^20.16.13",
    "@vitest/coverage-v8": "^2.1.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "niceai-scripts": "workspace:*",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "turbo": "^2.2.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": "^18.19.0 || >=20.0.0",
    "pnpm": "^9.4.0"
  }
}
